<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>	
	<package name="Publiclogin" extends="abstract_struts" namespace="/">		
		<action name="Logout" class="action.Logout">
			<result name="logout">/index.jsp</result>
		</action>		
	</package>	
	
	<package name="fee" extends="abstract_struts" namespace="/">		
		<action name="TuitionManager" class="action.fee.TuitionManagerAction">
			<result>/jsp/fee/TuitionManager.jsp</result>
		</action>		
		<action name="PayAccountManager" class="action.fee.PayAccountManager">
			<result>/jsp/fee/PayAccountManager.jsp</result>
		</action>
		<action name="PayInfoManager" class="action.fee.PayInfoManagerAction">
			<result>/jsp/fee/PayInfoManager.jsp</result>
		</action>
		
	</package>
	
	<package name="Score" extends="abstract_struts" namespace="/">			
		
		<action name="ScoreViewManager" class="action.score.ScoreViewManagerAction">
			<result>/jsp/score/ScoreViewManager.jsp</result>
		</action>
		
		<action name="ScoreMasterManager" class="action.score.ScoreMasterManagerAction">
			<result>/jsp/score/ScoreMasterManager.jsp</result>
		</action>		
		
		<action name="ScorePrint" class="action.score.ScorePrintAction">
			<result>/jsp/score/ScorePrint.jsp</result>
		</action>
		
		<action name="ScoreFilder" class="action.score.ScoreFilderAction">
			<result>/jsp/score/ScoreFilder.jsp</result>
		</action>
		
		<action name="ScoreManager" class="action.score.ScoreManagerAction">
			<result>/jsp/score/ScoreManager.jsp</result>
		</action>
	</package>
	
	<package name="Student" extends="abstract_struts" namespace="/">
		<action name="DumpNewStmd" class="action.student.DumpNewStmdAction">
			<result>/jsp/student/DumpNewStmd.jsp</result>
		</action>
		
		<action name="CheckStmdProfile" class="action.student.CheckStmdProfileAction">
			<result>/jsp/student/CheckStmdProfile.jsp</result>			
		</action>
		
		<action name="Upgrade" class="action.student.UpgradeAction">
			<result>/jsp/student/Upgrade.jsp</result>
		</action>
		
		<action name="CountGstmd" class="action.student.CountGstmd">
			<result>/jsp/student/CountGstmd.jsp</result>
		</action>
		
		<action name="ClassManager" class="action.student.ClassManager">
			<result>/jsp/student/ClassManager.jsp</result>
		</action>
		
		<action name="CheckStmdGraduate" class="action.student.CheckStmdGraduateAction">
			<result>/jsp/student/CheckStmdGraduate.jsp</result>
			<result name="edit">/jsp/student/CheckStmdGraduate/edit.jsp</result>
		</action>
		
		<action name="UploadStmdImage" class="action.student.UploadStmdImageAction">
			<result>/jsp/student/UploadStmdImage.jsp</result>			
		</action>
		
	</package>
	
	<package name="PublicPrint" extends="abstract_struts" namespace="/">
		<action name="StdClasCount" class="action.print.StdClasCountPrint"/>
		
		<action name="DocPrint" class="action.print.DocPrintAction">
			<result>/jsp/DocPrint.jsp</result>
		</action>
	</package>
	
	<package name="Enroll" extends="abstract_struts" namespace="/">		
		<action name="EnrollManager" class="action.enroll.EnrollManager">
			<result>/jsp/enroll/EnrollManager.jsp</result>
		</action>
		<action name="EnrollView" class="action.enroll.EnrollViewAction">
			<result>/jsp/enroll/EnrollView.jsp</result>
		</action>
		
		
		
	</package>
	
</struts>